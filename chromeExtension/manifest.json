{ 
    "manifest_version": 3,
    "name": "YoutubeVideoSummarizer",
    "description": "Summarized transcripts for YouTube videos.",
    "version": "1.0",
    "permissions": ["activeTab"],

    "action": {
        "default_popup": "popup.html"
    },

    "background": {
        "service_worker": "serviceWorker.js"
    }

}
